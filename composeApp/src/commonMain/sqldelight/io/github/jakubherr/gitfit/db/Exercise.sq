
CREATE TABLE IF NOT EXISTS Exercise (
    exerciseId INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    description TEXT
);

insert:
INSERT OR REPLACE INTO Exercise(name, description)
VALUES (?, ?);

update:
UPDATE Exercise SET name = ?, description = ? WHERE exerciseId = ?;

selectAll:
SELECT * FROM Exercise;

selectById:
SELECT * FROM Exercise WHERE exerciseId = :id;

delete:
DELETE FROM Exercise WHERE exerciseId = ?;