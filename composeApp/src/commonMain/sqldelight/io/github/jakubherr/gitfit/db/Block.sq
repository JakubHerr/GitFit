
CREATE TABLE IF NOT EXISTS Block(
    blockId INTEGER PRIMARY KEY AUTOINCREMENT,
    workoutFk INTEGER,
    exerciseFk INTEGER NOT NULL,
    FOREIGN KEY(workoutFk) REFERENCES Workout(workoutId) ON DELETE CASCADE,
    FOREIGN KEY(exerciseFk) REFERENCES Exercise(exerciseId) ON DELETE CASCADE
);

insert:
INSERT OR REPLACE INTO Block(workoutFk, exerciseFk)
VALUES (?, ?);

deleteById {
    DELETE FROM Block WHERE blockId = :id;
}

getForWorkout:
SELECT * FROM Block WHERE workoutFk = :id;